    "@storybook/addon-links": "^7.4.5",
    "@storybook/blocks": "^7.4.5",
    "@storybook/testing-library": "^0.2.1",
    "@storybook/vue3": "^7.4.5",
    "@storybook/vue3-vite": "^7.4.5",
    "@testing-library/vue": "^7.0.0",
    "@unocss/reset": "^0.55.7",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitest/coverage-v8": "^0.34.6",
